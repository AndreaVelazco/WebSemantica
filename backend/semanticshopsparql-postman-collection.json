    {
      "name": "üîç SPARQL - Consultas Personalizadas",
      "item": [
        {
          "name": "Query: Productos m√°s caros",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?producto ?nombre ?precio\\nWHERE {\\n  ?producto :nombre ?nombre .\\n  ?producto :precio ?precio .\\n}\\nORDER BY DESC(?precio)\\nLIMIT 10\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Clientes con m√°s pedidos",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?cliente ?nombre (COUNT(?pedido) as ?totalPedidos)\\nWHERE {\\n  ?cliente :nombre ?nombre .\\n  ?cliente :realizoPedido ?pedido .\\n}\\nGROUP BY ?cliente ?nombre\\nORDER BY DESC(?totalPedidos)\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Productos m√°s vendidos",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?producto (COUNT(?pedido) as ?vecesVendido)\\nWHERE {\\n  ?pedido :contieneProducto ?producto .\\n}\\nGROUP BY ?producto\\nORDER BY DESC(?vecesVendido)\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Ventas totales por marca",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?marca (SUM(?monto) as ?ventaTotal)\\nWHERE {\\n  ?pedido :contieneProducto ?producto .\\n  ?producto :tieneMarca ?marca .\\n  ?pedido :montoTotal ?monto .\\n}\\nGROUP BY ?marca\\nORDER BY DESC(?ventaTotal)\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Pedidos entregados",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?pedido ?producto ?estado ?monto\\nWHERE {\\n  ?pedido :estadoPedido ?estado .\\n  ?pedido :contieneProducto ?producto .\\n  ?pedido :montoTotal ?monto .\\n  FILTER (?estado = 'Entregado')\\n}\\nORDER BY DESC(?monto)\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Clientes Premium (inferido)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\\n\\nSELECT ?cliente ?nombre (COUNT(?pedido) as ?totalPedidos)\\nWHERE {\\n  ?cliente rdf:type :ClientePremium .\\n  ?cliente :nombre ?nombre .\\n  ?cliente :realizoPedido ?pedido .\\n}\\nGROUP BY ?cliente ?nombre\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Productos Apple con iOS",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?producto ?nombre ?precio\\nWHERE {\\n  ?producto :tieneMarca :Apple .\\n  ?producto :tieneCaracteristica :iOS .\\n  ?producto :nombre ?nombre .\\n  ?producto :precio ?precio .\\n}\\nORDER BY ?precio\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Productos con stock bajo",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?producto ?nombre ?stock ?precio\\nWHERE {\\n  ?producto :nombre ?nombre .\\n  ?producto :stock ?stock .\\n  ?producto :precio ?precio .\\n  FILTER (?stock < 15)\\n}\\nORDER BY ?stock\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: An√°lisis de ventas por mes",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT (MONTH(?fecha) as ?mes) (COUNT(?pedido) as ?totalPedidos) (SUM(?monto) as ?ventaTotal)\\nWHERE {\\n  ?pedido :fechaPedido ?fecha .\\n  ?pedido :montoTotal ?monto .\\n}\\nGROUP BY (MONTH(?fecha))\\nORDER BY ?mes\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
        },
        {
          "name": "Query: Clientes por marca preferida",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"PREFIX : <http://www.semanticshop.com/ontology#>\\n\\nSELECT ?marca (COUNT(?cliente) as ?totalClientes)\\nWHERE {\\n  ?cliente :tieneMarcaPreferida ?marca .\\n}\\nGROUP BY ?marca\\nORDER BY DESC(?totalClientes)\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/sparql/query",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["api", "sparql", "query"]
            }
          },
          "response": []
